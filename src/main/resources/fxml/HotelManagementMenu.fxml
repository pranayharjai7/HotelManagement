<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pranayharjai.hotelmanagement.Controllers.HotelManagementMenuController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="850.0" opacity="0.75" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/primary.jpg" />
         </image>
      </ImageView>
      <AnchorPane fx:id="roomSearchAnchorPane" layoutX="62.5" layoutY="83.0" onMouseClicked="#roomSearchClicked" onMouseEntered="#roomSearchAnchorPaneMouseEntered" onMouseExited="#roomSearchAnchorPaneMouseExited" prefHeight="200.0" prefWidth="200.0" style="-fx-background-radius: 40;">
         <children>
            <ImageView fx:id="roomSearchImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/RoomSearch.png" />
               </image>
            </ImageView>
            <Label fx:id="roomSearchLabel" alignment="CENTER" layoutY="150.0" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #00ddff; -fx-border-color: #ff3f32; -fx-border-width: 2;" text="Room Search">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="checkInAnchorPane" layoutX="325.0" layoutY="83.0" onMouseClicked="#checkInClicked" onMouseEntered="#checkInAnchorPaneMouseEntered" onMouseExited="#checkInAnchorPaneMouseExited" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="checkInImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true">
               <image>
                  <Image url="@../Images/checkIn.png" />
               </image>
            </ImageView>
            <Label fx:id="checkInLabel" alignment="CENTER" layoutY="149.0" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #00ddff; -fx-border-color: #ff9d3d; -fx-border-width: 2;" text="Check In">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="checkOutAnchorPane" layoutX="587.5" layoutY="83.0" onMouseClicked="#checkOutClicked" onMouseEntered="#checkOutAnchorPaneMouseEntered" onMouseExited="#checkOutAnchorPaneMouseExited" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="checkOutImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true">
               <image>
                  <Image url="@../Images/CheckOut.jpg" />
               </image>
            </ImageView>
            <Label fx:id="checkOutLabel" alignment="CENTER" layoutY="150.0" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #00ddff; -fx-border-color: Black; -fx-border-width: 2;" text="Check Out">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="petManagementAnchorPane" layoutX="150.0" layoutY="374.0" onMouseClicked="#petManagementClicked" onMouseEntered="#petManagementAnchorPaneMouseEntered" onMouseExited="#petManagementAnchorPaneMouseExited" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="petManagementImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true">
               <image>
                  <Image url="@../Images/PetManagement.jpg" />
               </image>
            </ImageView>
            <Label fx:id="petManagementLabel" alignment="CENTER" layoutY="150.0" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #00ddff; -fx-border-color: #ecacbc; -fx-border-width: 2;" text="Pet Mangement">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="billsAnchorPane" layoutX="500.0" layoutY="374.0" onMouseClicked="#billsCLicked" onMouseEntered="#billsAnchorPaneMouseEntered" onMouseExited="#billsAnchorPaneMouseExited" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="billsImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true">
               <image>
                  <Image url="@../Images/bill.jpg" />
               </image>
            </ImageView>
            <Label fx:id="billsLabel" alignment="CENTER" layoutY="150.0" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #00ddff; -fx-border-color: #111b58; -fx-border-width: 2;" text="Bills">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="755.0" layoutY="28.0" mnemonicParsing="false" onAction="#logoutButtonClicked" prefHeight="35.0" prefWidth="85.0" style="-fx-background-color: #F00000; -fx-border-color: black;" text="LOGOUT">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
